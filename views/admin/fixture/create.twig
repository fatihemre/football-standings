{% extends 'base.twig' %}
{% block title %}Fikstür Oluştur{% endblock %}
{% block content %}

    <div class="form-container">
        <h3 class="title">Yeni Fikstür Oluştur</h3>
        <form action="/manage/fixtures/create" method="post">

            <div class="input-row">
                <label for="display_name">Fikstür Adı:</label>
                <input type="text" id="display_name" name="display_name">
            </div>
            <div class="input-row">
                <label></label>
                <button type="submit">Kaydet</button>
            </div>
        </form>

    </div>


{% endblock %}